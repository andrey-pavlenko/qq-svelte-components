{
  "name": "qq-svelte-components",
  "version": "0.1.0",
  "scripts": {
    "start": "sirv public --port 8080",
    "build": "npm run build:application-first",
    "dev": "npm run dev:application-first",
    "validate": "svelte-check",
    "build:application-first": "rimraf public/application-first && rollup -c src/application-first/rollup.config.js",
    "dev:application-first": "rimraf public/application-first && rollup -w -c src/application-first/rollup.config.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^1.0.3",
    "svelte": "^3.0.0",
    "svelte-check": "^0.1.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {}
}
